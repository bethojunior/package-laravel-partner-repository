{
    "name": "bethojunior/partnerrepository",
    "description": "Laravel command to create applications in the partner repository",
    "type": "composer-plugin",
    "version": "1.0.0",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "bethojunior\\partnerrepository\\": "src/"
        }
    },
    "authors": [
        {
            "name": "bethojunior",
            "email": "hgomescostajr@gmail.com"
        }
    ],
    "scripts": {
